pipeline{
    agent none
    stages{
        stage ('BUILD') {
            agent { label 'masterkeynode' }

            steps{
                sh "echo Hi "
            }
        }
        
        stage ('Test') {
            parallel{
                stage('Deploy'){
                agent { label 'masterkeynode' }
                   steps{
                    sh "pwd"
                }
                }
                stage('Test'){
                agent { label 'masterkeynode' }

                    steps{
                        sh "echo hi"
                    }
                }
            }

        }
        stages{
        stage ('Deploy') {
            agent { label 'node-cproject' }

            steps{
                sh "echo Hi "
            }
        }


    }
    }
}
