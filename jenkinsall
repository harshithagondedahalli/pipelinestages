pipeline{
    agent none
    stages {
                stage ('BUILD') {
            agent { label 'masterkeynode' }

            steps{
                sh "echo Hi "
            }
        }
        
        stage ('Test') {
            parallel{
                stage ('Deploy'){
                agent { label 'masterkeynode' }
                   steps{
                    sh "pwd"
                }
                }
                stage ('test'){
                agent { label 'masterkeynode' }

                    steps{
                        sh "echo hi"
                    }
                }
            }
        }
        
        
        stage ('DEPLOY') {
            agent { label 'node-cproject' }

            steps{
                sh "echo Hi "
            }
        }

        
    }
 }
