pipeline{
    agent none
    stage{
        stages ('BUILD') {
            agent { label 'masterkeynode' }

            steps{
                sh "echo Hi "
            }
        }
        
        stages ('Test') {
            parallel{
                stages ('Deploy'){
                agent { label 'masterkeynode' }
                   steps{
                    sh "pwd"
                }
                }
                stages ('test'){
                agent { label 'masterkeynode' }

                    steps{
                        sh "echo hi"
                    }
                }
            }
        }
        }
        stages{
        stage ('DEPLOY') {
            agent { label 'node-cproject' }

            steps{
                sh "echo Hi "
            }
        }


    }
 }
